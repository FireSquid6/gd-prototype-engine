; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "PlatformController2D",
"language": "GDScript",
"path": "res://addons/prototyper/controllers/platform_controller.gd"
}, {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://addons/prototyper/state_machine/state.gd"
}, {
"base": "Node",
"class": "StateMachine",
"language": "GDScript",
"path": "res://addons/prototyper/state_machine/state_machine.gd"
} ]
_global_script_class_icons={
"PlatformController2D": "",
"State": "",
"StateMachine": ""
}

[application]

config/name="Gd-prototype-engine"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/prototyper/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
